{
  "family": "ConfigRedeploy",
  "taskRoleArn": "arn:aws:iam::445803720301:role/EcsTaskStarpractice",
  "volumes": [
    {
      "name": "efs-vol",
      "host": {
        "sourcePath": "/EFS"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "ConfigRedeploy",
      "image": "445803720301.dkr.ecr.us-west-2.amazonaws.com/renatbek/config:latest",
      "memory": 200,
      "cpu": 200,
      "essential": true,
      "portMappings": [],
      "mountPoints": [
        {
          "sourceVolume": "efs-vol",
          "containerPath": "/EFS"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "sparkydots_config",
          "awslogs-region": "us-west-2"
        }
      },
      "volumesFrom": [],
      "environment": [],
      "extraHosts": [],
      "ulimits": [],
      "dockerLabels": {}
    }
  ]
}

